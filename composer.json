{
    "name": "shedcollective/command-line-tool",
    "version": "1.15.0",
    "homepage": "https://shedcollective.com",
    "description": "The command line tool for simplifying life at Shed",
    "license": "MIT",
    "authors": [
        {
            "name": "Pablo de la PeÃ±a",
            "email": "p@shedcollective.org"
        }
    ],
    "support": {
        "email": "hello@shedcollective.org"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-zip": "*",
        "ext-curl": "*",
        "bamarni/composer-bin-plugin": "1.*",
        "symfony/console": "6.*",
        "symfony/finder": "6.*",
        "symfony/yaml": "6.*",
        "toin0u/digitalocean-v2": "5.*",
        "kriswallsmith/buzz": "1.*",
        "google/apiclient": "2.*",
        "aws/aws-sdk-php": "3.*",
        "phpseclib/phpseclib": "3.*",
        "monolog/monolog": "3.*",
        "psr/cache": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "10.*",
        "phpstan/phpstan": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Shed\\Cli\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@composer bin all install --ansi"
        ],
        "post-update-cmd": [
            "@composer bin all update --ansi"
        ],
        "build": [
            "./scripts/build.sh"
        ],
        "test": [
            "./scripts/test.sh"
        ],
        "analyse": [
            "./scripts/analyse.sh"
        ]
    },
    "bin": [
        "dist/shed"
    ],
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "forward-command": true
        }
    }
}
