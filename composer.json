{
    "name": "shedcollective/command-line-tool",
    "version": "1.0.7",
    "homepage": "https://shedcollective.com",
    "description": "The command line tool for simplifying life at Shed",
    "license": "MIT",
    "authors": [
        {
            "name": "Pablo de la Pe√±a",
            "email": "p@shedcollective.org"
        }
    ],
    "support": {
        "email": "hello@shedcollective.org"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "bamarni/composer-bin-plugin": "1.2.0",
        "symfony/console": "4.2.2",
        "symfony/finder": "4.2.2",
        "symfony/yaml": "4.2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.1"
    },
    "autoload": {
        "psr-4": {
            "Shed\\Cli\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": ["@composer bin all install --ansi"],
        "post-update-cmd": ["@composer bin all update --ansi"],
        "build" : ["./scripts/build.sh"],
        "test" : ["./scripts/test.sh"]
    },
    "bin": [
        "dist/shed"
    ]
}
