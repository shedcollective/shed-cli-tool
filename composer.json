{
    "name": "shedcollective/command-line-tool",
    "version": "1.3.2",
    "homepage": "https://shedcollective.com",
    "description": "The command line tool for simplifying life at Shed",
    "license": "MIT",
    "authors": [
        {
            "name": "Pablo de la Pe√±a",
            "email": "p@shedcollective.org"
        }
    ],
    "support": {
        "email": "hello@shedcollective.org"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-zip": "*",
        "ext-curl": "*",
        "bamarni/composer-bin-plugin": "1.2.0",
        "symfony/console": "4.2.2",
        "symfony/finder": "4.2.2",
        "symfony/yaml": "4.2.2",
        "toin0u/digitalocean-v2": "^2.2",
        "kriswallsmith/buzz": "^0.15.2",
        "google/apiclient": "^2.0",
        "aws/aws-sdk-php": "^3.95"
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.1"
    },
    "autoload": {
        "psr-4": {
            "Shed\\Cli\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@composer bin all install --ansi"
        ],
        "post-update-cmd": [
            "@composer bin all update --ansi"
        ],
        "build": [
            "./scripts/build.sh"
        ],
        "test": [
            "./scripts/test.sh"
        ]
    },
    "bin": [
        "dist/shed"
    ]
}
