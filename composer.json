{
    "name": "shedcollective/command-line-tool",
    "version": "1.14.5",
    "homepage": "https://shedcollective.com",
    "description": "The command line tool for simplifying life at Shed",
    "license": "MIT",
    "authors": [
        {
            "name": "Pablo de la PeÃ±a",
            "email": "p@shedcollective.org"
        }
    ],
    "support": {
        "email": "hello@shedcollective.org"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-zip": "*",
        "ext-curl": "*",
        "bamarni/composer-bin-plugin": "^1.4",
        "symfony/console": "5.*",
        "symfony/finder": "5.*",
        "symfony/yaml": "5.*",
        "toin0u/digitalocean-v2": "^4.6",
        "kriswallsmith/buzz": "^1.2",
        "google/apiclient": "^2.0",
        "aws/aws-sdk-php": "^3.95",
        "phpseclib/phpseclib": "^3.0",
        "monolog/monolog": "2.7.0",
        "psr/cache": "1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Shed\\Cli\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@composer bin all install --ansi"
        ],
        "post-update-cmd": [
            "@composer bin all update --ansi"
        ],
        "build": [
            "./scripts/build.sh"
        ],
        "test": [
            "./scripts/test.sh"
        ],
        "analyse": [
            "./scripts/analyse.sh"
        ]
    },
    "bin": [
        "dist/shed"
    ],
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "php-http/discovery": true
        }
    }
}
